# To use this project, rename this file to .env and add your own values.

# Polygon Amoy Testnet RPC URL
AMOY_RPC_URL="https://rpc-amoy.polygon.technology"

# Your private key from your wallet (e.g., MetaMask).
# IMPORTANT: DO NOT COMMIT THIS FILE WITH YOUR REAL PRIVATE KEY.
PRIVATE_KEY="0xYOUR_PRIVATE_KEY"

# Your Polygonscan API key for contract verification.
# This is optional but required for auto-verification.
POLYGONSCAN_API_KEY="YOUR_POLYGONSCAN_KEY"
